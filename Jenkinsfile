pipeline {
    agent any
    stages {
        stage ('first build') {
            steps {
                echo "hello"
            }
        }
        stage ('second build') {
            steps {
                script {
                    def course = "zure"
                    if (course == "zure")    
                    println ("success ${course}")
                   
                    else {
                    print "no"}
                }
            }
        }
    }
}
